class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        n = len(nums)
        sum1 = n*(n+1)//2
        sum2 = 0
        for i in range(n):
            sum2 += nums[i]
        ans = sum1-sum2
        return ans


https://leetcode.com/problems/missing-number/submissions/1345305566/
