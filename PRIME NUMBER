num = 20
if num == 1 or num == 0:
    print('Not a PM')
for i in range(2,(num//2)+1):
    if num % i == 0:
        print("Not a PM")
        break
print("Prime")



import math
num = 20
prime_flag = 0
if num <= 1:
    print('False')
for i in range(2,int(math.sqrt(num))+1):
    if num % i == 0:
        prime_flag = 1
        break
if prime_flag == 0:
    print('True')
else:
    print('False')
